{
    "name": "vue-skeleton",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "lint": "eslint .",
        "lint-fix": "eslint . --fix",
        "prettier": "prettier --write \"**/*.+(vue|js|jsx|json|css|md)\"",
        "pretty": "npm run prettier && npm run lint-fix",
        "serve": "vue-cli-service serve",
        "build": "npm run pretty && vue-cli-service build"
    },
    "dependencies": {
        "core-js": "^3.6.5",
        "vue": "^3.0.0",
        "vue-router": "^4.0.8"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-eslint": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "@vue/compiler-sfc": "^3.0.0",
        "@vue/eslint-config-prettier": "^6.0.0",
        "babel-eslint": "^10.1.0",
        "eslint": "^6.7.2",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-vue": "^7.0.0",
        "prettier": "^2.2.1"
    }
}
